{
  "private": true,
  "name": "storefront-remix-starter",
  "description": "",
  "license": "",
  "scripts": {
    "build:cf": "cross-env CF_PAGES=1 remix build",
    "build:nf": "cross-env NETLIFY=1 remix build",
    "build": "tailwindcss -m -i ./tailwind.css -o app/styles/app.css && remix build",
    "dev:wrangler": "wrangler pages dev ./public",
    "dev:css": "tailwindcss -w -i ./tailwind.css -o app/styles/app.css",
    "dev": "PORT=3001 concurrently \"yarn run dev:css\" \"remix dev\"",
    "dev:cf": "npm run build:cf && concurrently \"remix watch\" \"npm run dev:wrangler\"",
    "start:cf": "cross-env NODE_ENV=production yarn run dev:wrangler",
    "start": "PORT=3001 NODE_ENV=production GA_TRACKING_ID=G-WXDXT03MK9 remix-serve build",
    "generate": "graphql-codegen --config codegen.yml",
    "prepare": "husky install"
  },
  "dependencies": {
    "@calcom/embed-react": "^1.0.12",
    "@graphql-codegen/typescript-generic-sdk": "^3.0.4",
    "@headlessui/react": "^1.7.4",
    "@heroicons/react": "^2.0.13",
    "@material-tailwind/react": "^2.1.1",
    "@netlify/functions": "^1.3.0",
    "@remix-run/cloudflare": "^1.11.1",
    "@remix-run/cloudflare-pages": "^1.11.1",
    "@remix-run/netlify": "^1.11.1",
    "@remix-run/node": "^1.11.1",
    "@remix-run/react": "^1.11.1",
    "@remix-run/serve": "^1.11.1",
    "@remix-run/server-runtime": "^1.11.1",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^1.53.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "bootstrap": "^5.2.3",
    "braintree-web-drop-in": "^1.33.7",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "isbot": "^3.7.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.11.1",
    "remix-auth": "^3.4.0",
    "remix-auth-socials": "^2.0.5",
    "remix-validated-form": "^4.6.9",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@remix-run/dev": "^1.11.1",
    "@types/braintree-web-drop-in": "^1.33.0",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "wrangler": "^2.4.2"
